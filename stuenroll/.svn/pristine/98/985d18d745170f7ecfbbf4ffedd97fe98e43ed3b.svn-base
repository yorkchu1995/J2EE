body {
  background-color: #e7e7e7;
}
.main-container {
  background-color: #fff;
  margin: 0.9375rem;
  padding: 0.9375rem;
}
.main-container .tab-list {
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #EBEEF7;
}
.main-container .tab-list .tab-item {
  float: left;
  height: 2.25rem;
  line-height: 2.25rem;
  text-align: center;
  padding-left: 1.25rem;
  padding-right: 1.25rem;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  cursor: pointer;
}
.main-container .tab-list .tab-active {
  color: #FFFFFF;
  background-color: #02A0E9;
}
.main-container .tab-container .tab-content {
  display: none;
}
.main-container .tab-container .tab-content .operation-list {
  margin: 0;
  padding: 0;
  height: 3rem;
  position: relative;
}
.main-container .tab-container .tab-content .operation-list .operation-item {
  float: left;
  height: 3rem;
  line-height: 3rem;
  text-align: center;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  padding-left: 0.625rem;
  padding-right: 0.625rem;
  cursor: pointer;
  display: none;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu {
  display: none;
  cursor: default;
  position: absolute;
  left: 0;
  top: 2.9375rem;
  background-color: #F0F0F0;
  color: #333;
  width: 100%;
  border: solid #e0e0e0 0.0625rem;
  box-shadow: 0 0.1875rem 0.25rem rgba(0, 0, 0, 0.18);
  padding: 1.25rem;
  z-index: 50;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-title {
  margin: 0;
  padding: 0;
  margin-left: 0.8125rem;
  text-align: left;
  height: 3.125rem;
  line-height: 3.125rem;
  margin-bottom: 0;
  padding-bottom: 0.625rem;
  font-size: 0.9375rem;
  letter-spacing: 0.0625rem;
  border-bottom: #ccc dashed 0.0625rem;
  font-weight: 700;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content {
  overflow: hidden;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-title-2 {
  float: left;
  padding: 0;
  padding-top: 1.33333333rem;
  text-align: left;
  padding-left: 0.9375rem;
  margin: 0;
  font-weight: 700;
  font-size: 0.875rem;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list {
  float: left;
  margin: 0 0 0 2.5rem;
  padding: 0;
  list-style: none;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(1) {
  overflow: hidden;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(1) * {
  float: left;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(1) .label {
  margin-top: 1.375rem;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(1) .keyword {
  width: 12.5rem;
  height: 2rem;
  margin-top: 1.25rem;
  letter-spacing: 0.0625rem;
  font-weight: 400;
  outline: none;
  margin-top: 1.875rem;
  margin-left: 0.9375rem;
  margin-right: 1.25rem;
  padding: 0.3125rem;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(1) .menu-btn {
  display: block;
  width: 7.5rem;
  height: 2rem;
  margin-bottom: 1.5625rem;
  margin-top: 1.875rem;
  margin-left: 1.5625rem;
  background-color: #F4494E;
  border: none;
  color: #fff;
  outline: none;
  border-radius: 0.1875rem;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  font-weight: 600;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(1) .menu-btn:active {
  background-color: #CD0C12;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(2) {
  overflow: hidden;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(2) * {
  float: left;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(2) .menu-title-3 {
  height: 100%;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(2) .checkbox-wrapper {
  width: 56.25rem;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(2) .checkbox-wrapper input {
  margin-top: 1rem;
  margin-left: 1.0625rem;
  width: 0.9375rem;
  height: 0.9375rem;
}
.main-container .tab-container .tab-content .operation-list .operation-item .menu .menu-content .menu-list .menu-list-item:nth-child(2) .checkbox-wrapper span {
  margin-right: 0.9375rem;
  color: #333;
  font-size: 0.875rem;
  font-weight: 400;
  margin-top: 1.125rem;
  line-height: 0.8125rem;
  text-align: center;
}
.main-container .tab-container .tab-content .surface {
  display: none;
}
.main-container .tab-container .tab-content .active {
  display: block;
}
.main-container .tab-container .tab-content .data-table td {
  width: 9.0625rem;
}
.main-container .tab-container .tab-content .data-table td:nth-child(1) {
  width: 3.9375rem;
}
.main-container .tab-container .tab-content .data-table td:nth-child(2) {
  width: 5.625rem;
}
.main-container .tab-container .tab-content .data-table td:nth-child(3) {
  width: 14.375rem;
}
.main-container .tab-container .tab-content .data-table td:nth-child(3) .text-overflow {
  width: 14.375rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.main-container .tab-container .tab-content .data-table .statusBtnBase {
  background-color: #00CC99;
  height: 1.25rem;
  width: 3.125rem;
  border-radius: 0.625rem;
  overflow: hidden;
  position: relative;
  cursor: pointer;
}
.main-container .tab-container .tab-content .data-table .statusBtnBase .statusCircus {
  float: left;
  background-color: #FFFFFF;
  width: 1.25rem;
  height: 1.25rem;
  border-radius: 100%;
  display: inline-block;
}
.main-container .tab-container .tab-content .data-table .statusBtnBase .statusText {
  font-weight: 600;
  color: #FFF;
  font-size: 0.75rem;
  float: left;
  display: inline-block;
  margin: 0.125rem 0.3125rem;
  padding: 0;
  cursor: pointer;
}
.main-container .tab-container .tab-content .data-table .statusBtnBase .statusCircusOff {
  position: absolute;
  left: 1.875rem;
}
.main-container .tab-container .tab-content .data-table .statusBtnBase .statusTextOff {
  position: absolute;
  left: 0.3125rem;
}
.main-container .tab-container .tab-content .data-table .statusBtnBaseOff {
  background-color: #FF6633;
}
.main-container .tab-container .content-active {
  display: block;
}
.main-container .tab-container .page-list {
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: ##EBEEF7;
  margin-top: 0.9375rem;
}
.main-container .tab-container .page-list .page-item {
  height: 2.5rem;
  font-size: 0.875rem;
  line-height: 2.5rem;
}
.main-container .tab-container .page-list .page-info {
  float: left;
  margin-left: 0.9375rem;
}
.main-container .tab-container .page-list .page-info span {
  margin-left: 0.3125rem;
  margin-right: 0.3125rem;
}
.main-container .tab-container .page-list .page-operation {
  float: right;
  margin-right: 0.625rem;
}
.main-container .tab-container .page-list .page-operation a {
  cursor: pointer;
  padding-left: 0.3125rem;
  padding-right: 0.3125rem;
}
.main-container .tab-container .page-list .page-operation a:hover {
  color: #ff6700;
}
.main-container .tab-container .page-list .page-operation .page-active {
  color: red;
  font-weight: 600;
}
.main-container .tab-container .page-list .page-operation .page-disable {
  color: #B9B9B9;
  cursor: default;
}
.main-container .tab-container .page-list .page-operation .page-disable:hover {
  color: #B9B9B9;
}
.input-error {
  color: red !important;
}
