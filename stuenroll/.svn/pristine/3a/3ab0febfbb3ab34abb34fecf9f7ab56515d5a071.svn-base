html {
  height: 100%;
}
body {
  background: url("../img/student.jpg");
  background-repeat: no-repeat;
  background-size: auto;
  height: 100%;
}
.main {
  width: 75rem;
  height: 100%;
  margin-left: auto;
  margin-right: auto;
  position: relative;
}
.main .login-container {
  width: 31.25rem;
  height: 26.25rem;
  background-color: rgba(255, 255, 255, 0.5);
  border: solid 0.3125rem rgba(255, 255, 255, 0.6);
  border-radius: 0.1875rem;
  position: absolute;
  top: 9.375rem;
  right: 1.25rem;
}
.main .login-container .login-list {
  margin: 0;
  padding: 0;
}
.main .login-container .login-list .login-item {
  text-align: center;
}
.main .login-container .login-list .login-item .title {
  font-size: 1.875rem;
  font-weight: 300;
  letter-spacing: 0.1875rem;
  line-height: 6.25rem;
  margin-bottom: 0.625rem;
}
.main .login-container .login-list .login-item .input {
  width: 21.875rem;
  height: 2.5rem;
  outline: none;
  background-color: rgba(255, 255, 255, 0.7);
  border-radius: 0.1875rem;
  border: solid #e9e9e9 0.0625rem;
  padding-left: 0.9375rem;
  padding-right: 0.9375rem;
  font-size: 0.875rem;
  letter-spacing: 0.0625rem;
  margin-bottom: 1.5625rem;
  color: #333;
}
.main .login-container .login-list .login-item .btn {
  width: 21.875rem;
  height: 2.5rem;
  outline: none;
  border: none;
  border-radius: 0.1875rem;
  font-size: 0.875rem;
  letter-spacing: 0.0625rem;
  font-weight: bold;
  cursor: pointer;
  margin-bottom: 1.5625rem;
}
.main .login-container .login-list .login-item .blue {
  background-color: #00B4EF;
  color: #fff;
}
.main .login-container .login-list .login-item .blue:active {
  background-color: #0AC2FF;
}
.main .login-container .login-list .login-item .link {
  color: #666;
  padding-bottom: 0.1875rem;
  border-bottom: solid 0.0625rem #666;
  cursor: pointer;
}
.main .recover-container {
  display: none;
  width: 48.75rem;
  height: 31.25rem;
  background-color: rgba(255, 255, 255, 0.95);
  border: solid 0.3125rem rgba(255, 255, 255, 0.6);
  border-radius: 0.1875rem;
  position: absolute;
  top: 9.375rem;
  left: 13.125rem;
  overflow: hidden;
}
.main .recover-container .title {
  font-size: 1.375rem;
  font-weight: 500;
  color: #fff;
  background-color: rgba(0, 180, 239, 0.75);
  padding-left: 1.25rem;
  padding-right: 1.25rem;
  line-height: 2.625rem;
  letter-spacing: 0.0625rem;
  margin-top: 1.875rem;
}
.main .recover-container .step-container .step-bar .step-list {
  margin: 2.8125rem auto 0;
  padding: 0;
  width: 45rem;
  height: 3.75rem;
}
.main .recover-container .step-container .step-bar .step-list .step-item {
  float: left;
  width: 11.25rem;
  height: 0.625rem;
  background-color: #ccc;
  position: relative;
}
.main .recover-container .step-container .step-bar .step-list .step-item .ball {
  position: absolute;
  width: 2.125rem;
  height: 2.125rem;
  border: solid 0.25rem #b9b9b9;
  background-color: #999;
  border-radius: 100%;
  color: #fff;
  text-align: center;
  font-size: 0.75rem;
  line-height: 1.75rem;
  top: -0.75rem;
  left: 4.6875rem;
}
.main .recover-container .step-container .step-bar .step-list .step-item .desc {
  position: absolute;
  top: 1.75rem;
  font-size: 0.875rem;
  letter-spacing: 0.0625rem;
  text-align: center;
  width: inherit;
}
.main .recover-container .step-container .step-bar .step-list .step-active {
  background-color: #E86167;
}
.main .recover-container .step-container .step-bar .step-list .step-active .ball {
  background-color: #E86167;
  border-color: #EE8D91;
}
.main .recover-container .step-container .step-content {
  overflow: hidden;
}
.main .recover-container .step-container .step-content .step-wrapper {
  width: 192.5rem;
  overflow: hidden;
  margin-left: 0;
  transition: all 0.75s;
}
.main .recover-container .step-container .step-content .step-wrapper .step {
  width: 48.125rem;
  float: left;
  position: relative;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list {
  margin: 0 auto 0 auto;
  padding: 0;
  width: 40.625rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item {
  padding-top: 0.625rem;
  padding-bottom: 0.625rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item .step-title {
  font-size: 1.125rem;
  font-weight: 500;
  letter-spacing: 0.0625rem;
  border-bottom: dashed 0.0625rem #999;
  padding-bottom: 0.625rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item label {
  width: 4.6875rem;
  font-size: 0.875rem;
  letter-spacing: 0.0625rem;
  margin-right: 0.625rem;
  display: inline-block;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item .input {
  width: 18.75rem;
  height: 2.5rem;
  outline: none;
  border-radius: 0.1875rem;
  border: solid 0.0625rem #e0e0e0;
  color: #333;
  font-size: 0.875rem;
  letter-spacing: 0.0625rem;
  padding-left: 0.9375rem;
  padding-right: 0.9375rem;
  margin-right: 0.625rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item .btn {
  width: 9.375rem;
  height: 2.5rem;
  border-radius: 0.1875rem;
  border: none;
  font-weight: bold;
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
  outline: none;
  cursor: pointer;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item .green {
  color: #fff;
  background-color: #0EC093;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item .green:active {
  background-color: #10D8A5;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item .red {
  color: #fff;
  background-color: #E86167;
}
.main .recover-container .step-container .step-content .step-wrapper .step .content-list .content-item .red:active {
  background-color: #EB777C;
}
.main .recover-container .step-container .step-content .step-wrapper .step .success-list {
  margin: 3.125rem auto 0 auto;
  padding: 0;
  width: 40.625rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .success-list .success-item {
  text-align: center;
}
.main .recover-container .step-container .step-content .step-wrapper .step .success-list .success-item .success-title {
  font-size: 1.5rem;
  font-weight: 600;
  color: #0CA881;
  letter-spacing: 0.0625rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .success-list .success-item .success-desc {
  font-size: 1rem;
  letter-spacing: 0.0625rem;
  margin-bottom: 1.25rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .success-list .success-item .success-desc .second {
  color: #E86167;
  font-weight: bold;
  margin-right: 0.3125rem;
}
.main .recover-container .step-container .step-content .step-wrapper .step .success-list .success-item .back-btn {
  width: 9.375rem;
  height: 2.5rem;
  background-color: #0CA881;
  color: #fff;
  letter-spacing: 0.0625rem;
  outline: none;
  border: none;
  border-radius: 0.1875rem;
  font-size: 0.875rem;
  font-weight: bold;
}
.main .recover-container .step-container .step-content .step-wrapper .step .QR {
  position: absolute;
  top: 3.125rem;
  left: 4.375rem;
}
.main .error {
  color: red !important;
}
/*# sourceMappingURL=login.css.map */
