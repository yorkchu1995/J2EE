.delete-enroll-modal {
    width: 400px;
    height: 180px;
    border-radius:5/16rem;
    .title {
        font-size: 20/16rem;
        text-align: center;
        color: #F15761;
        letter-spacing: 1/16rem;
        padding-left: 20/16rem;
        padding-right: 20/16rem;
    }
    .message {
        text-align: center;
        margin: 0;
        color: #666;
        letter-spacing: 1/16rem;
        padding-left: 20/16rem;
        padding-right: 20/16rem;
        margin-bottom: 30/16rem;
    }
    .btn {
        width: 100%;
        border: none;
        background-color: #3EB358;
        border-bottom-left-radius: 5/16rem;
        border-bottom-right-radius: 5/16rem;
        height: 60/16rem;
        color: #fff;
        font-size: 16/16rem;
        letter-spacing: 1/16rem;
        font-weight: bold;
    }
}