body { 
    min-width: 700/16rem;
    background-color: #e7e7e7;
}
.main-container {
    background-color: #fff;
    margin: 15/16rem;
    padding: 15/16rem;
    .tab-list {
        margin: 0;
        padding: 0;
        overflow: hidden;
        background-color: #EBEEF7;
        .tab-item { 
            float: left;
            height: 36/16rem;
            line-height: 36/16rem;
            text-align: center;
            padding-left: 20/16rem;
            padding-right: 20/16rem;
            letter-spacing: 1/16rem;
            font-size: 14/16rem;
            cursor: pointer;
        }
        .tab-active {
            color: #fff;
            background-color: #02A0E9;
        }
    }
    .tab-container {
//        overflow: hidden;
        .tab-content {
//          display: none;
            .operation-list {
                margin: 0;
                padding: 0;
                height: 48/16rem;
                position: relative;
                .operation-item {
                    float: left;
                    height: 48/16rem;
                    line-height: 48/16rem; 
                    text-align: center;
                    letter-spacing: 1/16rem;
                    font-size: 14/16rem;
                    padding-left: 10/16rem;
                    padding-right: 10/16rem;
                    cursor: pointer;
//                  display:none;
                    -moz-user-select: none;
                    -ms-user-select: none;
                    -webkit-user-select: none; 
                    .import-menu{
                        display: none;
                        cursor: default;
                        position: absolute;
                        left: 0;
                        top: 30/16rem;
//                      left: 30/16rem;
//                      top: 97/16rem;
                        background-color: #F0F0F0;
                        color: #333;
                        border: solid #e0e0e0 1/16rem;
                        box-shadow: 0 3/16rem 4/16rem rgba(0,0,0,0.18);
                        padding: 20/16rem;
                        z-index: 50;
                        width: 96.5%;
                        .menu-title {
                            margin: 0;
                            padding: 0;
                            margin-left: 13/16rem;
                            text-align: left;
                            height: 50/16rem;
                            font-size: 15/16rem;
                            letter-spacing: 1/16rem;
                            border-bottom: #CCC dashed 1/16rem;
                            font-weight: 700;
                        }
                        #upload {
                            overflow: hidden;
                            margin-top: 20/16rem;
                            margin-left: 13/16rem;
                            .file {
                                float: left;
                                width: 140/16rem;
                                height: 39/16rem;
                                border: 0;
                                overflow: hidden;
                                background-color: #08AFFD;
                                cursor: pointer;
                                border-radius: 3/16rem;
                                .select-file {
                                    overflow: hidden;
                                    height: 39/16rem;
                                    border: 0;
                                    border-radius: 3/16rem;
                                    .icon-text {
                                        float: left;
                                        color: #FFFFFF;
                                        font-size: 15/16rem;
                                        line-height: 16/16rem;
                                        text-align: center;
                                        letter-spacing: 1/16rem;
                                        margin-left: 5/16rem;
                                        margin-top: 11/16rem;
                                        font-family: "微软雅黑";
                                    }
                                    .jia {
                                        float: left;
                                        color: #FFFFFF;
                                        width: 20/16rem;
                                        height: 20/16rem;
                                        line-height: 19/16rem;
                                        font-size: 14/16rem;
                                        padding-left: 1/16rem;
                                        text-align: center;
                                        border-radius: 100%;
                                        cursor: pointer;
                                        margin-left: 20/16rem;
                                        margin-top: 9/16rem;
                                    }
                                    &:hover {
                                        background-color: #02A0EA;
                                    }
                                    &:active {
                                        background-color: #028FD1;
                                    }
                                }
                            }
                            .file-path {
                                float: left;
                                margin-left: 20/16rem;
                                border-radius: 1/16rem;
                                height: 39/16rem;
                                width: 400/16rem;
                                border: solid 1/16rem #d0d0d0;
                                border-radius: 1/16rem;
                                outline: none;
                                line-height: 20/16rem;
                                float: left;
                                font-size: 14/16rem;
                                letter-spacing: 1/16rem;
                                padding: 0 15/16rem;
                            }
                            .upload-file {
                                float: left;
                                height: 39/16rem;
                                border: 0;
                                color: #FFFFFF;
                                width: 120/16rem;
                                line-height: 39/16rem;
                                letter-spacing: 1/16rem;
                                font-size: 14/16rem;
                                font-family: "微软雅黑";
                                background-color: #08AFFD;
                                margin-left: 20/16rem;
                                border-radius: 3/16rem;
                                cursor: pointer;
                                &:hover {
                                    background-color: #02A0EA;
                                }
                                &:active {
                                    background-color: #028FD1;
                                }
                            }
                        }
                    }
                    .export-menu {
                        display: none;
                        cursor: default;
                        position: absolute;
                        left: 0;
                        top: 30/16rem;
                        background-color: #F0F0F0;
                        color: #333;
                        border: solid #e0e0e0 1/16rem;
                        box-shadow: 0 3/16rem 4/16rem rgba(0,0,0,0.18);
                        padding: 20/16rem;
                        z-index: 50;
                        width: 96.5%;
                        .menu-title {
                            margin: 0;
                            padding: 0;
                            margin-left: 13/16rem;
                            text-align: left;
                            height: 50/16rem;
                            font-size: 15/16rem;
                            letter-spacing: 1/16rem;
                            border-bottom: #CCC dashed 1/16rem;
                            font-weight: 700;
                        }
                        .menu-content {
                            overflow: hidden;
                            margin-left: 14/16rem;
                            margin-top: 20/16rem;
                            input {
                                float: left;
                                border: 0;
                                color: #FFFFFF;
                                font-size: 14/16rem;
                                letter-spacing: 1/16rem;
                                font-weight: 500;
                                width: 150/16rem;
                                height: 30/16rem;
                                outline: none;
                                margin-right: 30/16rem;
                                background-color: #02A0E9;
                                cursor: pointer;
                                border-radius: 2/16rem;
                                &:hover {
                                    background-color: #028FD0;
                                }
                                &:active {
                                    background-color: #027EB7;
                                }
                            }
                        }
                    }
                    .menu{
                        background-color: rgba(240,240,240,1);
                        position: absolute;
                        left: 0;
                        width: 100%;
                        z-index: 50;
                        border: none;
                        box-shadow: 0 3/16rem 4/16rem rgba(0,0,0,0.17);
                        display: none;
                        .menu-list{
                            margin: 0;
                            padding: 0;
                            .menu-item{                                
                                text-align: left;
                                padding-left:30/16rem ;
                                padding-right: 30/16rem;
                                letter-spacing: 1/16rem;
                                .menuTitle{
                                    margin-top: 10/16rem;
                                    margin-bottom: 0;
                                    font-weight: 800;
                                    font-size: 15/16rem;
                                    outline: none;
                                    border-bottom:#ccc dashed 1/16rem;
                                }
                                .sub-title{
                                    padding-right: 25/16rem;
                                    display: inline-block;
                                    font-size: 14/16rem;
                                    letter-spacing: 1/16rem;
                                    margin-top: 10/16rem;
                                }
                                label{
                                    display: inline-block;
                                    letter-spacing: 1/16rem;
                                    padding-right: 5/16rem;
                                    font-size: 14/16rem;
                                }
                                .input{
                                    height: 30/16rem;
                                    outline: none;
                                    padding: 16/16rem 10/16rem;
                                    letter-spacing: 1/16rem;
                                    border-radius: 3/16rem;
                                    border: solid #ccc 1/16rem;
//                                  margin-right: 15/16rem;
                                    font-size: 14/16rem;
                                    font-weight: ;
                                }
                                .btn{
                                    height: 32/16rem;
                                    width: 120/16rem;
                                    outline: none;
                                    background-color: #F4494E;
                                    color: #fff;
                                    font-size: 14/16rem;
                                    font-weight: 700;
                                    letter-spacing: 1/16rem;
                                    border: none;
                                    margin: 0 30/16rem;
                                    border-radius: 3/16rem;
                                    &:active{
                                        background-color: #CD0C12;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
    .error{
        color: red !important;
    }
}