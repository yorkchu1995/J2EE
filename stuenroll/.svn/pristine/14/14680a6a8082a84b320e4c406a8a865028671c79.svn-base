body {
  background-color: #e7e7e7;
}
.main-container {
  margin: 0.625rem;
}
.main-container .review-container {
  float: left;
  background-color: #fff;
  padding-top: 0.625rem;
  padding-bottom: 0.625rem;
  width: 56.25rem;
}
.main-container .review-container .title {
  margin-left: 2.5rem;
  margin-bottom: 0.25rem;
  letter-spacing: 0.5rem;
  font-weight: 700;
  font-size: 1.75rem;
  width: 56.25rem;
}
.main-container .review-container .info-container {
  margin-left: 2.5rem;
  width: 56.25rem;
  position: relative;
}
.main-container .review-container .info-container .bigPeat {
  background-color: #3399FF;
  width: 50rem;
  height: 1rem;
}
.main-container .review-container .info-container .peat-1 {
  background-color: #999999;
  height: 0.375rem;
  width: 50rem;
  margin-top: 0.25rem;
}
.main-container .review-container .info-container .peat-2 {
  background-color: #3399FF;
  height: 0.375rem;
  width: 50rem;
  margin-top: 0.25rem;
}
.main-container .review-container .info-container .student {
  list-style: none;
  width: 56.25rem;
}
.main-container .review-container .info-container .student .details {
  padding-bottom: 0.9375rem;
}
.main-container .review-container .info-container .student .details tr td {
  letter-spacing: 0.0625rem;
  font-size: 0.875rem;
}
.main-container .review-container .info-container .student .details tr td li {
  margin-top: 1.5625rem;
  margin-right: 0.625rem;
  float: left;
}
.main-container .review-container .info-container .student .details tr td li .date {
  width: 3.4375rem !important;
}
.main-container .review-container .info-container .student .details tr td li input[type="text"] {
  border: none;
  border-bottom: solid 0.0625rem black;
  width: 17.1875rem;
  font-size: 1rem;
}
.main-container .review-container .info-container .question {
  margin: 0;
  padding: 0;
}
.main-container .review-container .info-container .question .part {
  border-right: solid 0.0625rem #797979;
  height: 31.25rem;
  width: 6.25rem;
  padding-top: 14.0625rem;
  padding-left: 1.5625rem;
  margin-right: 2.1875rem;
  float: left;
}
.main-container .review-container .info-container .question .part .ball {
  border-radius: 100%;
  background-color: #3399FF;
  width: 3.125rem;
  height: 3.125rem;
  color: #FFFFFF;
  text-align: center;
  padding-top: 0.625rem;
  font-size: 1.25rem;
  font-weight: 500;
}
.main-container .review-container .info-container .question .questions {
  margin-bottom: 6.25rem;
  font-size: 0.875rem;
  width: 56.25rem;
}
.main-container .review-container .info-container .question .questions .list {
  list-style: none;
}
.main-container .review-container .info-container .question .questions .list .question {
  float: left;
  padding-right: 1.25rem;
}
.main-container .review-container .info-container .question .questions .list .answer span {
  padding-right: 0.625rem;
}
.main-container .review-container .info-container .question .questions .list .answer textarea {
  height: 11.25rem;
  margin-top: 0.625rem;
  width: 37.5rem;
}
.main-container .review-container .warning {
  margin-top: 1.125rem;
  margin-bottom: 1.125rem;
  padding-left: 9.375rem;
  color: #999999;
  width: 56.25rem;
}
.main-container .charts-container {
  float: left;
  width: 40.625rem;
  padding-bottom: 0.625rem;
  padding-left: 1.25rem;
}
.main-container .charts-container .phone-container .student {
  background-color: #4D4D65;
  height: 7.8125rem;
  position: relative;
}
.main-container .charts-container .phone-container .student .photo {
  float: left;
}
.main-container .charts-container .phone-container .student .photo img {
  width: 8.125rem;
  height: 8.125rem;
  border-radius: 100%;
  position: absolute;
  top: 2.1875rem;
  left: 1.5625rem;
  border: solid 0.375rem #fff;
  z-index: 50;
}
.main-container .charts-container .phone-container .student .info {
  float: left;
  color: #fff;
  position: absolute;
  left: 10.625rem;
  top: 3.125rem;
}
.main-container .charts-container .phone-container .student .info span[name='name'] {
  font-size: 1.875rem;
  font-weight: 600;
}
.main-container .charts-container .phone-container .student .tel {
  color: #fff;
  float: left;
  position: absolute;
  top: 2.5rem;
  right: 1.875rem;
}
.main-container .charts-container .phone-container .student .tel span {
  font-size: 2.8125rem;
  font-weight: 600;
}
.main-container .charts-container .phone-container .call {
  background-color: #fff;
  height: 7.8125rem;
  position: relative;
}
.main-container .charts-container .phone-container .call .time {
  position: absolute;
  top: 2.8125rem;
  left: 6.25rem;
  text-align: center;
}
.main-container .charts-container .phone-container .call .time span {
  font-size: 2.5rem;
  font-weight: 600;
  color: black;
}
.main-container .charts-container .phone-container .call .time .timer {
  font-size: 0.875rem !important;
  font-weight: 300 !important;
  color: black;
}
.main-container .charts-container .phone-container .call .callBtn {
  background-color: #FF0066;
  width: 9.375rem;
  height: 3.125rem;
  border-radius: 0.375rem;
  color: #FFFFFF;
  position: absolute;
  top: 2.8125rem;
  right: 6.25rem;
  text-align: center;
}
.main-container .charts-container .phone-container .call .callBtn span {
  display: none;
  font-size: 1.375rem;
  font-weight: 400;
  height: 3.125rem;
  line-height: 3.125rem;
}
.main-container .charts-container .phone-container .call .callBtn .item-active {
  display: block;
}
.main-container .charts-container .audio-container {
  height: 12.5rem;
}
