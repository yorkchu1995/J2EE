body {
    margin: 0;
    padding: 0;
}
#uploadImageForm {
    overflow: hidden;
    margin-top: 20/16rem;
    margin-left: 13/16rem;
    .selectFile {
        float: left;
        width: 179/16rem;
        height: 179/16rem;
        overflow: hidden;
        cursor: pointer;
        .preView {
            width: 179/16rem;
            height: 179/16rem;
        }
    }
}
.save {
    height: 39/16rem;
    border: 0;
    color: #FFFFFF;
    width: 150/16rem;
    line-height: 39/16rem;
    letter-spacing: 1/16rem;
    font-size: 14/16rem;
    font-family: "微软雅黑";
    background-color: #59CF4C;
    margin-left: 20/16rem;
    border-radius: 3/16rem;
    cursor: pointer;
    &:hover {
        background-color: #47CA39;
    }
    &:active {
        background-color: #3EB831;
    }
}