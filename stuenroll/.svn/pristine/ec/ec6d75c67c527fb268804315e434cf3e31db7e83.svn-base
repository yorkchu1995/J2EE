                                float: left;
                                border: 0;
                                color: #FFFFFF;
                                font-size: 14/16rem;
                                letter-spacing: 1/16rem;
                                font-weight: 500;
                                width: 150/16rem;
                                height: 30/16rem;
                                outline: none;
                                margin-right: 30/16rem;
                                background-color: #02A0E9;
                                cursor: pointer;
                                border-radius: 2/16rem;
                                &:hover {
                                    background-color: #028FD0;
                                }
                                &:active {
                                    background-color: #027EB7;
                                }
                            }
                        }
                    }
                }
            }
            .data-table {
                td {
                    &:nth-child(1) {
                        width: 60/16rem;
                    }
                    &:nth-child(2) {
                        width: 99/16rem;
                    }
                    &:nth-child(3) {
                        width: 199/16rem;
