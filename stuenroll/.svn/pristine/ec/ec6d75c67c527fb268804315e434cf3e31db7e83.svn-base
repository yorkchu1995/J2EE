                            .mainModifyUserMenu {
                                flex-grow: 1;
                                height: 1487/16rem;
                                background-color: #FFFFFF;
                                margin-top: 15/16rem;
                                margin-left: 15/16rem;
                                .title-1 {
                                    padding-top: 45/16rem;
                                    padding-left: 55/16rem;
                                    overflow: hidden;
                                    span {
                                        float: left;
